#!/usr/bin/env zsh
# Author - Haron Prime
# License WTFPL  http://www.wtfpl.net/

PID1=`ps aux | grep XMMemAll | grep -v grep | awk '{print $2}'`
PID2=`ps aux | grep XMRomNew | grep -v grep | awk '{print $2}'`
if [[ -n $PID1 ]]|[[ -n $PID2 ]]; then
    kill $PID1
    kill $PID2
    echo > /tmp/haron/MU
    echo > /tmp/haron/RU
    exit 0
else
    XMMemAll &
    XMRomNew &
    exit 0
fi
