#!/usr/bin/env zsh
# Author - Haron Prime
# License WTFPL  http://www.wtfpl.net/

PID1=`ps aux | grep XMVolNew | grep -v grep | awk '{print $2}'`
PID2=`ps aux | grep XMPlay   | grep -v grep | awk '{print $2}'`
if [[ -n $PID1 ]]||[[ -n $PID2 ]]; then
    kill $PID1 &
    kill $PID2 &
    echo > /tmp/haron/VL
    echo > /tmp/haron/PL
    exit 0
else
    XMVolNew &
    XMPlay &
    exit 0
fi
exit 0
