MW1=/tmp/$USER/MW1
MW2=/tmp/$USER/MW
F1=`cat $MW1`
F2=$(( $F1 + 1 ))
if [[ $F2 -eq 1 ]]; then 
    W="window"
else 
    W="windows"
fi
echo $(( $F1 + 1 )) > $MW1 
echo "<fc=#fa0>  <fn=5>ï‹’</fn> <fn=1>$F2 minimized $W</fn></fc>" > $MW2
exit 0
