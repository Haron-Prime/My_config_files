if [[ -n $(ps aux | grep XMDate | grep -v XMDateToggle | grep -v grep | awk '{print $2}') ]]; then
    kill $(ps aux | grep XMDate | grep -v XMDateToggle | grep -v grep | awk '{print $2}') &
    echo > /tmp/haron/DO &
fi
XMTimeToggle &
XMNetToggle &
XMTrafToggle &
XMCPUToggle &
XMTempToggle &
XMMemToggle &
XMVolToggle &
XMGWToggle &
exit 0
